{
    "name": "khodemobin/laravel-api-logger",
    "description": "Laravel package for api logs which can be used in debugging.",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "stable",
    "authors": [{
        "name": "mobin",
        "email": "mobinyazdanparast9@gmail.com"
    }],
    "require": {
        "php": "^8.0|^7.3",
        "jenssegers/agent": "dev-master",
        "laravel/framework": "^8.39"
    },
    "autoload": {
        "psr-4": {
            "LaravelApiLogger\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelApiLogger\\Providers\\LaravelApiLoggerServiceProvider"
            ]
        }
    }
}
