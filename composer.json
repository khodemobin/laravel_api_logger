{
    "name": "laravel_api_logger",
    "description": "Laravel package for api logs which can be used in debugging.",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "stable",
    "authors": [{
        "name": "mobin",
        "email": "mobinyazdanparast9@gmail.com"
    }],
    "require": {
        "php": ">=7.0",
        "whichbrowser/parser": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelApiLogger\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelApiLogger\\Providers\\LaravelApiLoggerServiceProvider"
            ]
        }
    }
}
